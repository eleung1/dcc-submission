<div class="row-fluid page-header">
  <div class="pull-left">
    <h1><small><a href="/releases/{{release}}">{{release}} /</a></small> {{projectName}}</h1>
  </div>
  <div class="pull-right">
    {{submission_action state}}
  </div>
</div>
<div class="row-fluid">
  <div class="span6 well well-small">
    State: <strong class="{{lowercase state}}">{{underscore2space state}}</strong><br>
    Last Updated: <small>Some date</small>
  </div>
</div>
<ul class="nav nav-tabs">
  <li class="active"><a href="#files" data-toggle="tab">Files</a></li>
  {{#if report}}
  <li><a href="#report" data-toggle="tab">Report</a></li>
  {{/if}}
</ul>
<div class="tab-content">
  <div class="tab-pane active" id="files">Files</div>
  <div class="tab-pane" id="report">
    <div id="report-container"></div>
  </div>
</div>
